{
  "name": "fazt-community-web",
  "description": "fazt-community-web",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "start": "node __sapper__/build",
    "dev": "run-p scss:watch sapper:dev",
    "build": "run-s scss:build css:build sapper:build",
    "export": "run-s scss:build css:build sapper:export",
    "format": "prettier --write src/**/*.{svelte,js,json}",
    "prelint": "npm run format",
    "lint": "eslint --fix src/**/*.{svelte,js}",
    "sapper:dev": "sapper dev",
    "sapper:build": "sapper build",
    "sapper:export": "sapper export",
    "scss:watch": "sass src/styles/main.scss static/global.css --watch --no-stop-on-error",
    "scss:build": "sass src/styles/main.scss static/global.css --no-source-map",
    "css:build": "postcss static/global.css -o static/global.css"
  },
  "dependencies": {
    "compression": "1.7.4",
    "polka": "0.5.2",
    "sirv": "1.0.12"
  },
  "devDependencies": {
    "@babel/core": "7.14.3",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-runtime": "7.14.3",
    "@babel/preset-env": "7.14.4",
    "@babel/runtime": "7.14.0",
    "@rollup/plugin-alias": "3.1.2",
    "@rollup/plugin-babel": "5.3.0",
    "@rollup/plugin-commonjs": "19.0.0",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-node-resolve": "13.0.0",
    "@rollup/plugin-replace": "2.4.2",
    "cssnano": "5.0.4",
    "eslint": "8.18.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-svelte3": "4.0.0",
    "npm-run-all": "4.1.5",
    "postcss": "8.3.0",
    "postcss-cli": "8.3.1",
    "prettier": "2.7.1",
    "prettier-plugin-svelte": "2.7.0",
    "rollup": "2.50.3",
    "rollup-plugin-svelte": "7.1.0",
    "rollup-plugin-svelte-svg": "0.2.3",
    "rollup-plugin-terser": "7.0.2",
    "sapper": "0.29.1",
    "sass": "1.34.0",
    "svelte": "3.38.2",
    "svelte-preprocess": "4.7.3",
    "svelte-scrollto": "0.2.0"
  }
}
